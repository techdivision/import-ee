{
    "name": "techdivision/import-ee",
    "description": "Pacemaker library providing basic Magento 2 EE import functionality",
    "license": "OSL-3.0",
    "require": {
        "php": ">=7.1.0",
        "techdivision/import": "16.x-dev as 15.2.1"
    },
    "require-dev": {
        "doctrine/dbal": "2.5.*",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "~3.4.0",
        "consolidation/robo": "~1.0",
        "mikey179/vfsstream": "~1.0",
        "symfony/http-kernel": "~3.4"
    },
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TechDivision\\Import\\Ee\\": [
                "src/",
                "symfony/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechDivision\\Import\\Ee\\": [
                "tests/unit"
            ]
        }
    }
}
